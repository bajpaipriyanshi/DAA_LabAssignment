// linear_search

#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n, key, flag=0;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }
        cin>>key;
        for(int i=0;i<n;i++)
        {
            if(arr[i]==key)
            {
                cout<<"Present "<<i<<endl;
                flag++;
                break;
            }
        }    
            if(flag==0)
            cout<<"Not present"<<endl;
    }
    return 0;
}
