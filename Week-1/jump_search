#include<iostream>
#include<math.h>
using namespace std;
int search(int *arr, int n, int key)
{
    int jump=sqrt(n);
    int prev = 0; 
	while (arr[min(jump, n)-1] < key) 
	{ 
		prev = jump; 
		jump += sqrt(n); 
		if (prev >= n) 
			return -1; 
	} 
    while (arr[prev] < key) 
	{ 
		prev++;
		if (prev == min(jump, n)) 
			return -1; 
	} 
    if (arr[prev] == key) 
		return prev; 

	return -1; 
}
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n], key;
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }
        cin>>key;
        int res=search(arr, n, key);
        if(res==-1)
        cout<<"Not present"<<endl;
        else
        cout<<"Present "<<res<<endl;
    }
    return 0;
}
